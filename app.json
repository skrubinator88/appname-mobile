{
  "expo": {
    "name": "Gig-Chasers",
    "slug": "Gig-Chasers",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_ios.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon_ios.png",
      "bundleIdentifier": "com.example.gig-chasers",
      "supportsTablet": false,
      "config": {
        "googleMapsApiKey": "AIzaSyB8xVAMRa2hoq8lRXEG1wXG3v2yR1KkHsQ"
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "waze"
        ]
      }
    },
    "android": {
      "icon": "./assets/icon_android.png",
      "useNextNotificationsApi": true
    }
  }
}